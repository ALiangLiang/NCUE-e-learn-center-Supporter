{
   "name": "NCUE網路教學平台 Supporter",
   "description": "讓 Chrome 瀏覽器可以正常瀏覽 NCUE 網路教學平台的「開始上課」選單，順便破解鎖右鍵。",
   "content_scripts": [ {
	  "all_frames": true,
      "js": [ "main.js" ],
      "matches": [ "*://dlearn.ncue.edu.tw/learn/path/pathtree.php" ],
	  "run_at": "document_start"
   }, {
	  "all_frames": true,
      "js": [ "delDisablejs.js" ],
      "matches": [ "*://dlearn.ncue.edu.tw/learn/*" ],
	  "run_at": "document_end"
   }, {
	  "all_frames": true,
      "js": [ "bottom.js" ],
      "matches": [ "*://dlearn.ncue.edu.tw/learn/path/manifest.php" ],
	  "run_at": "document_end"
   }, {
	  "all_frames": true,
      "js": [ "indexBtn.js" ],
      "matches": [ "*://dlearn.ncue.edu.tw/", "*://dlearn.ncue.edu.tw/index.php" ],
	  "run_at": "document_end"
   }],
   "icons": {
      "128": "icon/icon128.png",
      "16": "icon/icon16.png",
      "48": "icon/icon48.png"
   },
   "manifest_version": 2,
   "content_security_policy": "script-src 'self' https://www.google.com https://checkout.google.com; object-src 'self'",
   "version": "1.0.1"
}